{
  "name": "triliza",
  "version": "1.1.7",
  "private": true,
  "homepage": "https://jger.github.io/triliza",
  "dependencies": {
    "nanoid": "^5.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native-web": "^0.21.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "REACT_APP_VERSION=$npm_package_version react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "semantic-release": "semantic-release",
    "postinstall": "patch-package"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^13.1.2",
    "gh-pages": "^6.3.0",
    "patch-package": "^8.0.1",
    "semantic-release": "^25.0.2"
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "webpack-dev-server": "^5.2.1",
    "js-yaml": "^4.1.1",
    "glob": "^11.1.0",
    "node-forge": "^1.3.2",
    "selfsigned": {
      "node-forge": "^1.3.2"
    },
    "sucrase": {
      "glob": "^11.1.0"
    },
    "npm": {
      "glob": "^11.1.0"
    },
    "@semantic-release/npm": {
      "npm": {
        "glob": "^11.1.0"
      }
    },
    "@npmcli/package-json": {
      "glob": "^11.1.0"
    },
    "@npmcli/map-workspaces": {
      "glob": "^11.1.0"
    },
    "cacache": {
      "glob": "^11.1.0"
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(nanoid)/)"
    ],
    "moduleNameMapper": {
      "^nanoid$": "<rootDir>/node_modules/nanoid/index.browser.js"
    }
  }
}
